<keymap version="1" name="Default for Windows copy" parent="$default">
  <action id="Back">
    <keyboard-shortcut first-keystroke="alt minus" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="alt w" />
  </action>
  <action id="CopyElement" />
  <action id="DatabaseView.CopyTable" />
  <action id="Forward">
    <keyboard-shortcut first-keystroke="alt equals" />
  </action>
  <action id="GotoClass">
    <keyboard-shortcut first-keystroke="alt n" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl b" />
    <mouse-shortcut keystroke="control button1" />
    <mouse-shortcut keystroke="button2" />
    <keyboard-shortcut first-keystroke="alt b" />
  </action>
  <action id="GotoImplementation">
    <keyboard-shortcut first-keystroke="shift alt b" />
  </action>
  <action id="GotoNextError">
    <keyboard-shortcut first-keystroke="alt k" />
  </action>
  <action id="GotoPreviousError">
    <keyboard-shortcut first-keystroke="alt i" />
  </action>
  <action id="GotoSymbol">
    <keyboard-shortcut first-keystroke="shift alt n" />
  </action>
  <action id="GotoTypeDeclaration" />
  <action id="Graph.ApplyCurrentLayout" />
  <action id="RecentFiles">
    <keyboard-shortcut first-keystroke="alt e" />
  </action>
  <action id="Run">
    <keyboard-shortcut first-keystroke="f5" />
  </action>
  <action id="tasks.goto" />
  <action id="tasks.open.in.browser" />
</keymap>